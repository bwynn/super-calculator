{
  "name": "ng_calc",
  "version": "1.0.0",
  "description": "This Angular Calculator application is a standalone angular application that running on Angular 1.5, with a Node and Express back end. There is no database attached to this application, as it is intended to be utilized on a case-by-case basis.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "./node_modules/.bin/karma start --browsers Firefox --single-run",
    "postinstall": "bower install"
  },
  "engines": {
    "node": "6.2.2"
  },
  "author": "Brian Wynn",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/bwynn/super-calculator.git"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-spec-reporter": "0.0.26"
  }
}
